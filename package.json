{
  "name": "staking-dashboard",
  "version": "0.0.2",
  "description": "Orbs, Stacking dashboard",
  "main": "index.js",
  "engines": {
    "node": "10.17.0"
  },
  "scripts": {
    "publish": "node release.js",
    "test": "npm run test-server && npm run test-client",
    "type-check": "tsc -p ./src/client/tsconfig.json",
    "test-server": "jest --config ./src/server/__tests__/jest.config.js",
    "test-server-watch": "jest --watch --verbose --config ./src/server/__tests__/jest.config.js",
    "test-client": "cross-env DISABLE_CANVAS=true jest --config ./src/client/__tests__/jest.config.js",
    "test-client-watch": "cross-env DISABLE_CANVAS=true jest --watch --config ./src/client/__tests__/jest.config.js",
    "test-e2e": "cross-env DISABLE_ANIMATIONS=true jest --config ./e2e/jest-e2e.config.js",
    "clean": "rimraf dist",
    "postinstall": "npm run build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "node --trace-warnings index.js",
    "start:prod": "NODE_ENV=production node index.js",
    "build-client": "cross-env NODE_ENV=production webpack -p",
    "dev-client": "webpack-dev-server -w",
    "build-server": "tsc -p ./src/server",
    "dev-server": "tsc-watch -p ./src/server --onSuccess \"node index.js\" --noClear",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\""
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/orbs-network/staking-dashboard.git"
  },
  "license": "MIT",
  "author": "Gil Amran",
  "dependencies": {
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@types/find-up": "^2.1.1",
    "body-parser": "^1.18.3",
    "color": "^3.1.2",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "find-up": "^3.0.0",
    "format-number": "^3.0.0",
    "github-api": "^3.3.0",
    "gsap": "^2.1.3",
    "mobx": "^5.14.0",
    "mobx-react": "^6.1.4",
    "orbs-blocks-polling-js": "^2.0.0",
    "orbs-client-sdk": "^1.3.0",
    "orbs-pos-data": "^1.1.11",
    "postprocessing": "^6.6.0",
    "react-popper-tooltip": "^2.8.3",
    "rollbar": "^2.5.4",
    "styled-components": "^4.3.2",
    "three": "^0.125.0",
    "tslib": "^1.9.3",
    "twitter": "^1.7.1",
